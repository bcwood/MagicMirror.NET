<!DOCTYPE html>
<html lang="en">
<head>
	<meta charset="utf-8" />
	<meta name="viewport" content="width=device-width, initial-scale=1.0" />
	<title>@ViewData["Title"] - MagicMirror</title>

	<link href="https://fonts.googleapis.com/css2?family=Roboto:wght@100&display=swap" rel="stylesheet" />
	<link href="~/css/site.css?v=@DateTime.Now.Ticks" rel="stylesheet" />
	@RenderSection("Head", required: false)
</head>
<body>
	@RenderBody()

	<script src="~/lib/jquery-3.5.1.min.js"></script>
	<script src="https://kit.fontawesome.com/dde1b15fe1.js" crossorigin="anonymous"></script>
	<script src="~/js/site.js?v=@DateTime.Now.Ticks"></script>
	@RenderSection("Scripts", required: false)
</body>
</html>
